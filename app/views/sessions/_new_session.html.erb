    <%= form_for(:session, url: sessions_path, :html=>{class: "form-signin", role: "form"}) do |f| %>

      <%= f.email_field :email ,class: "form-control", placeholder: "Email address",autofocus: true %>

      <%= f.password_field :password ,class: "form-control", placeholder: "Password"%>

      <%= f.submit "Sign in", class: "btn btn-lg btn-success btn-block"%>
    <% end %>
